<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="za.co.msocios.gotrackadmin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.GoTrackAdmin">
        <activity
            android:name=".ChildDetails"
            android:exported="false"
            android:parentActivityName=".DashBoards"
            />
        <activity
            android:name=".DashBoards"
            android:exported="false"
            android:label="@string/title_activity_dash_boards"
            android:theme="@style/Theme.GoTrackAdmin.NoActionBar" />
        <activity
            android:name=".Dashboard"
            android:exported="false" />
        <activity
            android:name=".SignIn"
            android:exported="false"
            android:theme="@style/Theme.GoTrackAdmin.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/Theme.GoTrackAdmin.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>